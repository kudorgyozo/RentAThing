{
    "name": "rent-a-thing.client",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --ssl",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "___prestart": "node aspnetcore-https",
        "___start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
        "___start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
        "lint": "ng lint"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.1.6",
        "@angular/common": "^19.1.6",
        "@angular/compiler": "^19.1.6",
        "@angular/core": "^19.1.6",
        "@angular/forms": "^19.1.6",
        "@angular/platform-browser": "^19.1.6",
        "@angular/platform-browser-dynamic": "^19.1.6",
        "@angular/router": "^19.1.6",
        "jest-editor-support": "31.1.2",
        "run-script-os": "^1.1.6",
        "rxjs": "~7.8.1",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.1.7",
        "@angular/cli": "^19.1.7",
        "@angular/compiler-cli": "^19.1.6",
        "@types/jasmine": "~5.1.5",
        "angular-eslint": "19.1.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.20.1",
        "jasmine-core": "~5.4.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "postcss": "^8.5.2",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.7.3",
        "typescript-eslint": "8.23.0"
    }
}
